
<%- include("partials/header"); -%>
<div class="compose">


    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Your Name</label>
        <input class="form-control" type="text" name="nameBody">
        <label>Title</label>
        <input class="form-control" type="text" name="titleBody">
        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
</div>
<%- include("partials/footer"); -%>
